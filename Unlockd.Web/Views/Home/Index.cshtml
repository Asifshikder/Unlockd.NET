                        <a data-marker="1" data-aos="zoom-out"
                           data-aos-duration="2000" class="btn btn-primary btn-lg  btn-square btn-themecolor btn-block" data-toggle="modal" data-target="#modalPhoneUnlock" href="#">Unlock Phone</a>
                    </div>
                </div>
            </div>

        </div>
    </div>

</div>


<div class="body-content" style="background-color:#e0f6f3">
    <div class="unlock-online" style="padding-top:20px;">
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12 text-center">
                    <div class="choose-network-header">
                        <h3>Unlock your phone with VeeUnlock</h3>
                        <br />
                    </div>
                </div>
                <div class="col-md-6 col-xs-12">
                    <div class="unlock-online-func" style="background-color:white">
                        <div class="func-form">
                            <a class="btn btn-lg btn-primary btn-square btn-block btn-themecolor" href="#togglecarrier">Select carrier to unlock</a>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-xs-12">
                    <div class="unlock-online-func" style="background-color:white">
                        <div class="func-form">
                            <a class="btn btn-lg btn-primary btn-block btn-square btn-themecolor" href="#toggledevice">Select device to unlock</a>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<div class="device-network">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 text-center">
                <h2 class="device-network-header" data-aos="fade-up" *@
                    data-aos-duration="2000">
                    Unlock your device from any network Worldwide.
                </h2>
                <p class="device-network-subheader" data-aos="fade-down"
                   data-aos-duration="2000">In three simple steps you can permanently unlock you device.</p>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4 col-xs-12" data-aos="zoom-in"
                 data-aos-duration="2000">
                <div class="device-network-content text-center">
                    <div class="device-network-content-wrap">
                        <div class="device-network-counter">1</div>
                        <div class="device-network-details">
                            <h3>Enter your device details</h3>
                            <p>Select the model and network the device is locked to and enter the IMEI or Serial <i class="fa fa-question-circle hidden-xs" data-toggle="popover" data-placement="bottom" data-trigger="hover" style="font-size: 1.2em; position: relative; top: 1px; color: rgb(0,122,255); cursor: help;" data-content="Your IMEI number can be found in the Settings > General > About screen, or by dialing *#06#" title="" data-original-title="Finding your IMEI Number"></i></p>
