@{ int BFC = 0;}
@foreach (var item in Model.Brands)
{
    if (BFC == 0)
    {
        <div class="device">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6" data-aos="zoom-in"
                         data-aos-duration="2000">
                        <div class="device-content" style="padding-top:100px">
                            <p class="device-content-tag">Unlock your</p>
                            <h2 class="device-name">@item.Name Device</h2>
                            <h3 class="device-price"><span>From Only</span> $@item.AverageUnlockingPrice</h3>
                            <p>@item.ShortDescription</p>
                            <a href="/UnlockPhone/ByBrand?id=@item.Id" class="btn btn-primary btn-square black">Unlock Now</a>
                        </div>

                    </div>
                    <div class="col-sm-6" data-aos="zoom-in"
                         data-aos-duration="2000">
                        <img src="@Url.Content(item.FeaturedImage)" alt="@Url.Content(item.Name)" class="img-responsive">
                    </div>
                </div>
            </div>
        </div>


    }
    else if (BFC == 1)
    {

        <div class="device">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6" data-aos="zoom-in"
                         data-aos-duration="2000">
                        <img src="@Url.Content(item.FeaturedImage)" alt="@Url.Content(item.Name)" class="img-responsive">
                    </div>
                    <div class="col-sm-6" data-aos="zoom-in"
                         data-aos-duration="2000">
                        <div class="device-content" style="padding-top:100px">
                            <p class="device-content-tag">Unlock your</p>
                            <h2 class="device-name">@item.Name Phone</h2>
                            <h3 class="device-price"><span>From Only</span> $@item.AverageUnlockingPrice</h3>
                            <p>@item.ShortDescription</p>
                            <a href="/UnlockPhone/ByBrand?id=@item.Id" class="btn btn-primary btn-square black">Unlock Now</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    }
    else
    {
        <div class="device">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6" data-aos="zoom-in"
                         data-aos-duration="2000">
                        <div class="device-content" style="padding-top:100px">
                            <p class="device-content-tag">Unlock your</p>
                            <h2 class="device-name">@item.Name Device</h2>
                            <h3 class="device-price"><span>From Only</span> $@item.AverageUnlockingPrice</h3>
                            <p>@item.ShortDescription</p>
                            <a href="/UnlockPhone/ByBrand?id=@item.Id" class="btn btn-primary btn-square black">Unlock Now</a>
                        </div>

                    </div>
                    <div class="col-sm-6" data-aos="zoom-in"
                         data-aos-duration="2000">
                        <img src="@Url.Content(item.FeaturedImage)" alt="@Url.Content(item.Name)" class="img-responsive">
                    </div>
                </div>
            </div>
        </div>
    }
    BFC++;
}

