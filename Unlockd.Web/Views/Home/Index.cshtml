@model HomeContentsViewModel
@{
    ViewData["Title"] = "Unlock your phone";
}


<style>
    h1 {
        font-size: 105px;
    }

    html {
        scroll-behavior: smooth;
    }

    .hero-text {
        width: 100%;
        font-family: Montserrat, Helvetica, sans-serif !important;
        font-weight: 800;
        color: #22435c !important;
    }

    @@media screen and (max-width: 480px) {
        h1 {
            font-size: 30px !important;
            line-height: 80%;
            margin-top: 100px;
        }

        .hero-text {
            width: 100%;
            color: rgb(6 6 6);
            width: 100%;
            font-family: sans-serif !important;
            font-weight: 100;
            color: #22435c !important;
        }
    }
</style>

<header>
    <div class="carousel" data-count="1" data-current="1">

        <div class="items" style="margin-top:-40px">
            <div class="item active" data-marker="1" data-aos="fade-up"
                 data-aos-duration="2000">
                <img src="~/Contents/assets/img/mockup-863469_1920.jpg" style="animation: animatedBackground 10s linear infinite alternate;" alt="Background" class="background" data-marker="1" data-aos="zoom-in"
                     data-aos-duration="3000" data-aos-delay="4" />

                <div class="content">
                    <div class="outside-content">
                        <div class="inside-content">
                            <div class="container">
                                <div class="visible-xs hidden-sm hidden-md hidden-lg text-center">
                                    <h2>We unlock<br />Phones</h2>
                                </div>
                                <div class="row hidden-xs">
                                    <div class="col-sm-8 col-md-offset-2 align-center">
                                        @*<h3 class="h2 hidden-xs" data-marker="1" data-aos="fade-up"
                                                data-aos-duration="2000">
                                                <b>Unlock Your Phone With VeeUnlock</b>
                                            </h3>
                                            <p class="h3 hidden-xs" data-marker="1" data-aos="fade-down"
                                               data-aos-duration="2000"><b>Instant Remote Unlocking. Use Your Phone With Any Network</b></p>
                                            <p class="hidden-xs" style=" color: cadetblue;"><b>Unlock your phone using our online service. We unlock all phones using the IMEI number so that your device is capable of using any Networks sim card. Our recommended method of unlocking is safe and legal, No technical skills are required</b></p>*@

                                        <h1 class="hero-text" data-marker="1" data-aos="fade-down"
                                            data-aos-duration="2000">
                                            We Unlock<br />Phones
                                        </h1>
                                        <div class="">
                                            <a data-marker="1" data-aos="zoom-in"
                                               data-aos-duration="2000" class="btn btn-primary btn-lg btn-square  btn-themecolor  btn-style" href="#" data-toggle="modal" data-target="#modalcarriercheck">Carrier Check</a>
                                            <a data-marker="1" data-aos="zoom-out"
                                               data-aos-duration="2000" class="btn btn-primary btn-lg  btn-square btn-themecolor btn-style" href="#" data-toggle="modal" data-target="#modalPhoneUnlock">Unlock Phone</a>
                                        </div>


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <ul hidden class="markers">
            <li hidden data-marker="1" class="active"></li>
        </ul>
    </div>
</header>
<!-- Modal -->
<div class="modal fade" id="modalcarriercheck" tabindex="-1" role="dialog" aria-labelledby="modalcarriercheckTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="">
            <div class="modal-body" style="background-color:transparent">
                <div class="row">
                    <div class="col-md-7 col-md-offset-2  col-xs-12">
                        <div class="unlock-online-func" style="background-color:white">
                            <div class="">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="unlock-online-cart">
                                        <div class="cart-bg-img">
                                            <img src="~/Carrier/Mobile.png" />
                                        </div>
                                        <div class="online-cart-wrap">
                                            <div class="cart-wrap-upper text-center">
                                                <div class="cart-upper-img">
                                                    <div class="cart-upper-img-wrap">
                                                        <img src="~/images/PhoneCheck/Carriericon.png" />

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="cart-wrap-lower">
                                                <div class="cart-wrap-upper text-center" style="">
                                                    <h3 style="color:white">Carrier check</h3>

                                                </div>
                                                @await Component.InvokeAsync("CarrierCheck")

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalPhoneUnlock" tabindex="-1" role="dialog" aria-labelledby="modalPhoneUnlockTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="">
            <div class="modal-body" style="background-color:transparent">
                <div class="row">
                    <div class="col-md-7 col-md-offset-2  col-xs-12">
                        <div class="unlock-online-func" style="background-color:white">
                            <div class="">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="unlock-online-cart">
                                        <div class="cart-bg-img">
                                            <img src="~/Carrier/Mobile.png" />
                                        </div>

                                        <div class="online-cart-wrap">
                                            <div class="cart-wrap-upper text-center" style="margin-top:100px">
                                                <h3 style="color:white">Unlock phone</h3>
                                            </div>
                                            <div class="cart-wrap-lower">
                                                @await Component.InvokeAsync("ModalPhoneUnlock")
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="visible-xs">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="text-center">
                    <div style="margin:10px">
