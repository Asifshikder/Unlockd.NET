@model Unlockd.Domain.Entities.Services.BrandFAQ
@{
    ViewData["Title"] = "Delete";
}
@Html.AntiForgeryToken()

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h2>Brand FAQ</h2>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home /</a></li>
                    <li><a href="/Admin/BrandFAQ/index">Brand FAQ</a></li>
                </ol>
            </div>
        </div>
    </div>
</section>
<div class="content">
    <div class="animated fadeIn">
        <div class="card">
            <div class="row">

                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">Delete Brand FAQ</strong>
                        </div>
                        <div class="card-body">
                            <form asp-controller="BrandFAQ" asp-action="DeleteConfirm" method="post">

                                <div class="form-group">
                                    <label class=" form-control-label">Question</label>
                                    <input type="text" asp-for="Question" class="form-control" disabled>
                                </div>
                                @Html.HiddenFor(model => model.Id)
                                @Html.HiddenFor(model => model.Answer)
                                @Html.HiddenFor(model => model.SupportMenuId)

                                <div class="form-group">
                                    <button type="submit" class="btn btn-danger">Delete</button>
                                    <a href="/Admin/BrandFAQ/Index?id=@Model.SupportMenuId" class="btn btn-warning">Cancel</a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>